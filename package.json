{
  "name": "leah-ts",
  "version": "1.1.0",
  "description": "Leah's TypeScript boilerplate",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "run": "node out/index.js",
    "lint": "eslint src/**/*.ts",
    "fix": "eslint src/**/*.ts --fix",
    "build": "npm run lint && npm run tsc",
    "start": "npm run build && npm run run"
  },
  "_moduleAliases": {
    "~": "./out"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leahlundqvist/ts.git"
  },
  "author": "Leah Lundqvist",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leahlundqvist/ts/issues"
  },
  "homepage": "https://github.com/leahlundqvist/ts#readme",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "module-alias": "^2.2.2",
    "ololog": "^1.1.175"
  }
}
